generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Review {
  id         Int      @id @default(autoincrement())
  public_id  String   @unique @default(uuid())
  content    String
  author     String
  created_at DateTime @default(now())
}
